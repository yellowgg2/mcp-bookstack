{
  "name": "mcp-bookstack",
  "version": "1.2.0",
  "main": "build/main.js",
  "type": "module",
  "bin": {
    "mcp-bookstack": "./build/main.js"
  },
  "files": [
    "build",
    "locales"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/main.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/main.js",
    "start": "node build/main.js"
  },
  "author": "",
  "license": "MIT",
  "description": "MCP Server for BookStack with content generation features",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "axios": "^1.8.4",
    "dotenv": "^16.4.7",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "bookstack"
  ]
}